  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "chalk": "4.1.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "aniep": "^0.6.0",
    "chinese-parseint2": "^2.0.6",
    "lodash": "^4.17.21",
    "pathe": "^1.1.2"
  }
